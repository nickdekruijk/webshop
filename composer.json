{
    "name": "nickdekruijk/webshop",
    "keywords": ["php", "laravel", "webshop", "cart", "checkout"],
    "description": "Add a simple webshop to your Laravel project",
    "license": "MIT",
    "authors": [{
        "name": "Nick de Kruijk",
        "email": "git@nickdekruijk.nl"
    }],
    "minimum-stability": "dev",
    "require": {
        "bobey/geoip2-geolite2-composer": "^1.6",
        "geoip2/geoip2": "^2.10",
        "laravel/framework": "^7.0|^8.75.0|^9.0|^10.0|^11.0",
        "mledoze/countries": "^4.0",
        "mollie/laravel-mollie": "^2.8",
        "php": ">=7.2.0"
    },

    "autoload": {
        "psr-4": {
            "NickDeKruijk\\Webshop\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NickDeKruijk\\Webshop\\ServiceProvider"
            ],
            "aliases": {
                "Webshop": "NickDeKruijk\\Webshop\\WebshopFacade"
            }
        }
    }
}
